local function TYYBU_fake_script() -- TextButton_3.Script 
	local script = Instance.new('LocalScript', TextButton_3)

	function onButtonClicked()
	script.Parent.Parent.Parent.Parent.Parent.Press:play()
	script.Parent.ON.Visible = true
	script.Parent.OFF.Visible = false
	B = script.Parent.Parent.Parent.Parent.Parent.Effects.HDR:Clone()
	B2 = script.Parent.Parent.Parent.Parent.Parent.Effects.HDR2:Clone()
	B.Parent = game.Workspace.CurrentCamera
	B2.Parent = game.Workspace.CurrentCamera
	script.Parent.Script2.Disabled = false
	script.Disabled = true
	end
	
	script.Parent.MouseButton1Click:connect(onButtonClicked)
	
end
coroutine.wrap(TYYBU_fake_script)()
