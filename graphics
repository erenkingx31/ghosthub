local function LEZF_fake_script() -- Main.Script 
	local script = Instance.new('LocalScript', Main)

	function onButtonClicked()
	script.Parent.Parent.Press.PlaybackSpeed = 1
	script.Parent.Image = 'http://www.roblox.com/asset/?id=212860701'
	script.Parent.Parent.Parent.Parent.Character.Humanoid.WalkSpeed = 0
	script.Parent.Parent.Parent.Parent.Character.Humanoid.JumpPower = 0
	script.Parent.Parent.Press:play()
	B = script.Parent.Parent.Effects.SettingsBlur:Clone()
	B.Parent = game.Workspace.CurrentCamera
	script.Parent.SettingsFrame.Visible = true
	script.Parent.Script2.Disabled = false
	script.Disabled = true
	end
	
	script.Parent.MouseButton1Click:connect(onButtonClicked)
	
end
coroutine.wrap(LEZF_fake_script)()
local function IJVXWYF_fake_script() -- Main.Script2 
	local script = Instance.new('LocalScript', Main)

	function onButtonClicked()
	script.Parent.Parent.Press.PlaybackSpeed = 0.7
	script.Parent.Image = 'rbxassetid://3599164218'
	script.Parent.Parent.Parent.Parent.Character.Humanoid.WalkSpeed = 16
	script.Parent.Parent.Parent.Parent.Character.Humanoid.JumpPower = 50
	script.Parent.Parent.Press:play()
	game.Workspace.CurrentCamera.SettingsBlur:remove()
	script.Parent.SettingsFrame.Visible = false
	script.Parent.Script.Disabled = false
	script.Disabled = true
	end
	
	script.Parent.MouseButton1Click:connect(onButtonClicked)
end
coroutine.wrap(IJVXWYF_fake_script)()
local function PVMXS_fake_script() -- SettingsFrame.Script 
	local script = Instance.new('Script', SettingsFrame)

	game.StarterGui.ResetPlayerGuiOnSpawn = false
end
coroutine.wrap(PVMXS_fake_script)()
local function PYMTXEU_fake_script() -- TextButton_2.Script 
	local script = Instance.new('LocalScript', TextButton_2)

	function onButtonClicked()
	script.Parent.Parent.Parent.Parent.Parent.Press:play()
	script.Parent.ON.Visible = true
